{
	"server": {
		  "http_addr": ":4443",
      "tls_key_file" : "./cyc/key.pem",
      "tls_cert_file" : "./cyc/certificate.pem"
	},
	"trust_service": {
		"type": "remote",
		"hostname": "notarysigner",
		"port": "7899",
		"tls_ca_file": "./root-ca.crt",
		"key_algorithm": "ecdsa",
		"tls_client_cert" :"./notary-server.crt",
		"tls_client_key": "./notary-server.key"
	},
	"logging": {
		"level": "info"
	},
	"storage": {
		"backend": "mysql",
		"db_url": "server@tcp(mysql:3306)/notaryserver?parseTime=True"
	}
}
